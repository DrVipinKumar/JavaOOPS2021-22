
public class SwitchExample {

	public static void main(String[] args) {
		int number = 60;
		String size=null;
	    switch (number) {
	      case 29:
	        size = "Small";break;
	      case 20:
		     size = "Small";break;
	      case 42: 
	    	 size = "Medium"; break;

	     case 44:
	        size = "Medium";  break;

	      case 58:
	        size = "Large";  break;
	      case 55:
		        size = "Large";  break;
		     default: size="Size does not match";
	}
	    System.out.println(size);
}
}
