
public class SwitchExample {

	public static void main(String[] args) {
		int number = 60;
		String size=null;
	    switch (number) {
	      case 29, 20 ->   size = "Small";
	      case 42, 44 ->  size = "Medium";
	      case 58, 55 ->  size = "Large"; 
	      default -> size="Size does not match";
	}
	    System.out.println(size);
}
}
