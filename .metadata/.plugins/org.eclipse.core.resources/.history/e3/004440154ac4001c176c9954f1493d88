package LamdaExpression;
@FunctionalInterface
interface Addition{
	void sum();
}
public class LamdaEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
           Addition a =()->{
           System.out.println("2 + 2 is 4");
           System.out.println("This is final output");
           };
           a.sum();
	}

}
