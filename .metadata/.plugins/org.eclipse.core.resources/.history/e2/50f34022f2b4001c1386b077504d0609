abstract class Area {
	abstract double getArea();
}
class Circle extends Area {

	@Override
	double getArea() {
		// TODO Auto-generated method stub
		return 3.14*2*2;
	}
	
}
public class SealedClass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
         Area a =new Circle();
	}

}
